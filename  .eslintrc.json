{
    "extends": "airbnb-base",
    "env": {
      "browser": false,
      "node": true,
      "es2021": true
    },
    "parserOptions": {
      "ecmaVersion": "latest",
      "sourceType": "module"
    },
    "rules": {
      "no-console": "off",
      "import/extensions": ["error", "always"]
    }
  }
  